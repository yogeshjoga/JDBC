package advance_java;

import java.sql.*;

public class Student {

	public static void main(String [] args) {
		
		String jdbcDriver = "com.mysql.cj.jdbc.Driver";
		String userName = "root";
		String pwd = "root";
		String connectionUrl = "jdbc:mysql://localhost:3306/yogi";
		
		
		// this is for database statment use purpose
		String stdId = "11001";
		String stdName = "yogesh joga";
		String stdNum = "9110566354";
		
		try {
			Class.forName(jdbcDriver);
			Connection con = DriverManager.getConnection(connectionUrl,userName,pwd);
			
			
			String sql = "INSERT INTO studentjava (studentId, studentName, studentNum)"
					+ "VALUES (?,?,?)";
			
			PreparedStatement stmt = con.prepareStatement(sql);
			Statement.setString(1, stdId);
			Statement.setString(2)
		}
		catch(Exception e) {
		}
	}
}
